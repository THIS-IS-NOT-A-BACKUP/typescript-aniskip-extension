{
  "name": "typescript-aniskip-extension",
  "extensionName": "Aniskip",
  "version": "1.4.2",
  "description": "An extension which gives the option to skip anime opening and endings on various streaming sites",
  "main": "content_script.ts",
  "repository": "https://github.com/lexesjan/typescript-aniskip-extension",
  "contributors": [
    "Lexes Jan Mantiquilla <mantiquillal@gmail.com>",
    "Dabreadman <tany3@tcd.ie>"
  ],
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js",
    "build:chrome": "cross-env NODE_ENV=production BROWSER=chromium webpack --config ./webpack/webpack.prod.config.js",
    "build:firefox": "cross-env NODE_ENV=production BROWSER=firefox webpack --config ./webpack/webpack.prod.config.js",
    "clean": "run-p clean:build clean:modules",
    "clean:build": "rimraf rm -rf ./dist ./web-ext-artifacts ./aniskip-source.zip",
    "clean:modules": "rimraf -rf ./node_modules",
    "dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:chrome": "cross-env NODE_ENV=development BROWSER=chromium webpack --config ./webpack/webpack.dev.config.js --watch",
    "dev:firefox": "cross-env NODE_ENV=development BROWSER=firefox webpack --config ./webpack/webpack.dev.config.js --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "package:chrome": "run-s build:chrome zip:chrome",
    "package:firefox": "run-s build:firefox zip:firefox",
    "redux-devtools": "redux-devtools --hostname=localhost --port=8000",
    "start:chrome": "wait-on ./dist && cross-env BROWSER=chromium web-ext --config=web-ext.config.js run --target chromium",
    "start:dev:chrome": "yarn clean:build && run-p dev:chrome start:chrome",
    "start:dev:firefox": "yarn clean:build && run-p dev:firefox start:firefox",
    "start:firefox": "wait-on ./dist && cross-env BROWSER=firefox web-ext --config=web-ext.config.js run --target firefox-desktop",
    "start:prod:chrome": "run-s build:chrome start:chrome",
    "start:prod:firefox": "run-s build:firefox start:firefox",
    "zip": "run-s build:chrome zip:chrome build:firefox zip:firefox zip:source",
    "zip:chrome": "cross-env BROWSER=chrome web-ext --config=web-ext.config.js build",
    "zip:firefox": "cross-env BROWSER=firefox web-ext --config=web-ext.config.js build",
    "zip:source": "git archive -o aniskip-source.zip HEAD"
  },
  "devDependencies": {
    "@redux-devtools/cli": "^1.0.0-9",
    "@types/chrome": "^0.0.154",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/remote-redux-devtools": "^0.5.5",
    "@types/string-similarity": "^4.0.0",
    "@types/uuid": "^8.3.1",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^10.3.1",
    "chrome-webstore-upload-cli": "^1.2.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "schema-utils": "^3.1.1",
    "style-loader": "^3.2.1",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "web-ext": "^6.3.0",
    "web-ext-submit": "^6.3.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-copy-plugin": "^0.0.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "remote-redux-devtools": "^0.5.16",
    "string-similarity": "^4.0.4",
    "tailwindcss": "^2.2.7",
    "uuid": "^8.3.2",
    "webextension-polyfill-ts": "^0.26.0"
  },
  "packageManager": "yarn@3.0.1"
}
